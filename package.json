{
  "name": "muffin-cli",
  "description": "The command line utility for building sites using muffin",
  "version": "0.1.0",
  "author": "Leonard Lamprecht <mindrun@icloud.com> (http://leo.im)",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-runtime",
      "syntax-async-functions"
    ]
  },
  "bin": {
    "muffin": "dist/bin/index",
    "muffin-new": "dist/bin/new",
    "muffin-serve": "dist/bin/serve",
    "muffin-build": "dist/bin/build",
    "muffin-import": "dist/bin/import"
  },
  "bugs": {
    "url": "https://github.com/muffinjs/cli/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "broccoli": "^0.16.9",
    "broccoli-sane-watcher": "^1.1.4",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "dotenv": "^2.0.0",
    "fs-extra": "^0.28.0",
    "git-config": "0.0.7",
    "gulp-chmod": "^1.3.0",
    "inquirer": "^1.0.1",
    "livereload": "^0.4.1",
    "mongonaut": "^2.1.0",
    "mongoose": "^4.4.12",
    "ncp": "^2.0.0",
    "ora": "^0.2.1",
    "update-notifier": "^0.6.3"
  },
  "devDependencies": {
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-ext": "^1.0.0"
  },
  "engines": {
    "node": "latest"
  },
  "files": [
    "data",
    "template",
    "dist"
  ],
  "greenkeeper": {
    "emails": false,
    "ignore": [
      "koa-mount",
      "koa-sendfile"
    ]
  },
  "homepage": "http://muffin.cafe",
  "keywords": [
    "muffin",
    "command",
    "line",
    "utility"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muffinjs/cli.git"
  }
}
